@{
    ViewData["Title"] = "Không đủ quyền";
}
<head>
    <link rel="stylesheet" type="text/css" href="~/assets/styles/categories_styles.css">
    <link rel="stylesheet" href="~/as/assets/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/as/assets/css/owl.carousel.min.css">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/as/assets/css/bootstrap.min.css">

    <!-- Style -->
    <link rel="stylesheet" href="~/as/assets/css/style.css">
</head>
<div class="container product_section_container">
    <div class="row">
        <div class="col product_section clearfix">

            <!-- Breadcrumbs -->
            <div class="breadcrumbs d-flex flex-row align-items-center">
                <ul>
                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-controller="Account" asp-action="Login"><i class="fa fa-angle-right" aria-hidden="true"></i>Đăng nhập</a></li>
                </ul>
            </div>

            <!-- Main Content -->
            <div class="main_content_cart">
                        <div class="container">
                            <div class="row align-items-center justify-content-center">
                                <div class="col-md-6">
                            <h2>Bạn không có quyền truy cập trang này.</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
@section Scripts {
    @{
        <script src="~/assets/js/categories_custom.js"></script>
        <script src="~/assets/plugins/jquery-ui-1.12.1.custom/jquery-ui.js"></script>
        <script src="~/as/assets/js/jquery-3.3.1.min.js"></script>
        <script src="~/as/assets/js/popper.min.js"></script>
        <script src="~/as/assets/js/bootstrap.min.js"></script>
        <script src="~/as/assets/js/main.js"></script>
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}